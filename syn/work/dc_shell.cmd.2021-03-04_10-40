#@ - b + NET b + DIRECTION INPUT + USE SIGNAL + LAYER M2 ( 0 0 ) ( 56 286 ) + PLACED ( 237012 0 ) N ;
#@ - cin + NET cin + DIRECTION INPUT + USE SIGNAL + LAYER M2 ( 0 0 ) ( 56 286 ) + PLACED ( 1093836 0 ) N ;
#@ - sum + NET sum + DIRECTION OUTPUT + USE SIGNAL + LAYER M2 ( 0 0 ) ( 56 286 ) + PLACED ( 957948 0 ) N ;
#@ - cout + NET cout + DIRECTION OUTPUT + USE SIGNAL + LAYER M2 ( 0 0 ) ( 56 286 ) + PLACED ( 939556 0 ) N ;
