gui_zoom -window [gui_get_current_window -view] -factor 2.0
gui_zoom -window [gui_get_current_window -view] -factor 0.5
gui_zoom -window [gui_get_current_window -view] -rect {{-112.839 1308.499} {1427.851 -223.511}}
write_def -pins -macro ../outputs/adder.dct.def
write_def -pins -macro -output ../outputs/adder.dct.def
