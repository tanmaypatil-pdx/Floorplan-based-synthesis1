#@ # ------------------------------------------------------------------
#@ itcl::body iwidgets::Scrolledcanvas::create {args} {
#@     set retval [eval $itk_component(canvas) create $args]
#@     
#@     configure -autoresize $itk_option(-autoresize)
#@     
#@     return $retval
#@ }
#@ 
